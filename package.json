{
  "name": "react-jsonld-editor",
  "version": "1.0.0",
  "private": true,
  "description": "React component for editing JSON-LD",
  "main": "index.js",
  "bin": {
    "learn-shapes": "./lib/learn-shapes.js"
  },
  "scripts": {
    "lint": "eslint --ext .jsx src",
    "transpile": "babel src -d lib --source-maps inline",
    "prebuild": "npm -s run lint",
    "build": "npm -s run transpile",
    "presetup": "npm -s run build",
    "setup": "cd data && make -B vocabs && make",
    "css": "cssnext src/src.css src/style.css",
    "develop": "budo src --css src/style.css --live -- -t [ babelify --sourceMaps inline ] --extension=.jsx",
    "pretranspile-tests": "npm -s run build",
    "transpile-tests": "babel test -d lib/test",
    "pretest": "npm -s run transpile-tests",
    "test": "tape lib/test/*.js | tap-spec"
  },
  "author": "Ryan Shaw",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.2.0",
    "basscss": "^8.0.1",
    "basscss-background-colors": "^2.1.0",
    "basscss-border-colors": "^2.1.0",
    "basscss-colors": "^2.2.0",
    "budo": "^8.0.4",
    "cssnext": "^1.8.4",
    "immutablediff": "^0.4.2",
    "n3": "^0.4.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "immutable-jsonld": "^1.0.0",
    "language-tags": "^1.0.5",
    "react": "^0.14.7",
    "react-autosuggest": "^3.7.4",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "reselect": "^2.5.1",
    "validator": "^5.1.0"
  }
}
