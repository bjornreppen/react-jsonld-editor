{
  "name": "react-jsonld-editor",
  "version": "1.0.0",
  "private": true,
  "description": "React component for editing JSON-LD",
  "main": "index.js",
  "bin": {
    "learn-shapes": "./lib/learn-shapes.js"
  },
  "scripts": {
    "lint": "eslint --ext .jsx src",
    "css": "cssnext src/src.css src/style.css",
    "develop": "budo src --css src/style.css --live -- -t [ babelify --sourceMaps inline ] --extension=.jsx",
    "transpile": "babel src -d lib --source-maps inline",
    "build": "npm -s run lint && npm -s run transpile",
    "transpile-tests": "npm -s run build && babel test -d lib/test",
    "test": "npm -s run transpile-tests && tape lib/test/*.js | tap-spec"
  },
  "author": "Ryan Shaw",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.2.0",
    "basscss": "^8.0.1",
    "basscss-background-colors": "^2.1.0",
    "basscss-border-colors": "^2.1.0",
    "basscss-btn": "^1.1.1",
    "basscss-btn-primary": "^1.1.0",
    "basscss-btn-sizes": "^1.2.0",
    "basscss-forms": "^1.0.0",
    "budo": "^8.0.4",
    "cssnext": "^1.8.4",
    "immutablediff": "^0.4.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "immutable-jsonld": "^1.0.0",
    "language-tags": "^1.0.5",
    "n3": "^0.4.5",
    "react": "^0.14.7",
    "react-autocomplete": "^0.2.1",
    "react-dom": "^0.14.7",
    "validator": "^5.1.0"
  }
}
